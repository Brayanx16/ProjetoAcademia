        <div class="col-md-10 content">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Atualizar Dados do Funcionario
                </div>
                <div class="panel-body">
                    <form action="/update/funcionario/<%= data.idfuncionario %>" class="form-horizontal" method="POST">
                        <div class="form-group col-md-5">
                            <label class="col-md-2 control-label" for="nome">Nome</label>
                            <div class="col-md-10">
                                <input id="nome" value="<%= data.nome %>" type="text" name="nome" placeholder="Nome do usuário" class="form-control input-md" required="">
                            </div>
                        </div>
                        <!-- Text input -->
                        <div class="form-group col-md-5">
                            <label class="col-md-2 control-label" for="rg">RG</label>
                            <div class="col-md-10">
                                <input id="rg" value="<%= data.rg %>"  type="text" name="rg" maxlength="8"  placeholder="x.xxx.xxx"
                                    class="form-control input-md" required="">
                            </div>
                        </div>
                        <!-- Text input-->
                        <div class="form-group col-md-5">
                            <label class="col-md-2 control-label" for="cpf">CPF</label>
                            <div class="col-md-10">
                                <input id="cpf" value="<%= data.cpf %>"  type="text" name="cpf" maxlength="14" onkeypress= "this.value = FormataCPF(event)" placeholder="xxx.xxx.xxx-xx"
                                    class="form-control input-md" required="">
                            </div>
                        </div>
                        <!-- Text input -->
                        <div class="form-group col-md-5">
                          <label class="col-md-2 control-label" for="email">Email</label>
                          <div class="col-md-10">
                              <input id="email" value="<%= data.email %>"  name="email" type="email" placeholder="exemplo@email.com" class="form-control input-md" required="">
                          </div>
                        </div> 
                        <!-- Text input-->
                        <div class="form-group col-md-5">
                            <label class="col-md-2 control-label" for="telefone">Tel.</label>
                            <div class="col-md-10">
                                <input id="telefone" value="<%= data.telefone %>"  type="text" name="telefone" maxlength="20" onkeypress="this.value = FormataTelefone(event)" placeholder="(xx) x xxxx-xxxx"
                                    class="form-control input-md" required="">
                            </div>
                        </div>
                        <!-- Text input -->
                        <div class="form-group col-md-5">
                            <label class="col-md-2 control-label" for="cargo">Cargo</label>
                            <div class="col-md-10">
                                <input id="cargo" value="<%= data.cargo %>"  name="cargo" type="text"  placeholder="Cargo" class="form-control input-md" required="">
                            </div>
                        </div> 
                        
                        <div class="col-md-8">
                            <input type="submit" class="btn btn-primary" value="Confirmar">
                            <a href="/funcionario/listFun"><input type="button" class="btn btn-danger"  value="Cancelar"></a>
                        </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>