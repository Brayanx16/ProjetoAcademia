                        <div class="form-group col-md-5">
                            <label class="col-md-2 control-label" for="rg">RG</label>
                            <div class="col-md-10">
                                <input id="rg" type="text" name="rg" maxlength="8"  placeholder="x.xxx.xxx"
                                    class="form-control input-md" required="">
                            </div>
                        </div>
                        <!-- Text input-->
                        <div class="form-group col-md-5">
                            <label class="col-md-2 control-label" for="cpf">CPF</label>
                            <div class="col-md-10">
                                <input id="cpf" type="text" name="cpf" maxlength="14" onkeypress= "this.value = FormataCPF(event)" placeholder="xxx.xxx.xxx-xx"
                                    class="form-control input-md" required="">
                            </div>
                        </div>
                        <!-- Text input -->
                        <div class="form-group col-md-5">
                          <label class="col-md-2 control-label" for="email">Email</label>
                          <div class="col-md-10">
                              <input id="email" name="email" type="email" placeholder="exemplo@email.com" class="form-control input-md" required="">
                          </div>
                        </div> 
                        <!-- Text input-->
                        <div class="form-group col-md-5">
                            <label class="col-md-2 control-label" for="telefone">Tel.</label>
                            <div class="col-md-10">
